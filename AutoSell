            Player.Backpack:FindFirstChild("Pure Rokakaka").Parent = game:GetService("Workspace").Living[Player.Name]
            wait()
                game:GetService("Players").LocalPlayer.Character.RemoteEvent:FireServer("EndDialogue",{["NPC"] = "Merchant",["Option"] = "Option1",["Dialogue"] = "Dialogue5"})
        end
        if num11 == 25 then
            Player.Backpack:FindFirstChild("Mysterious Arrow").Parent = game:GetService("Workspace").Living[Player.Name]
            wait()
                game:GetService("Players").LocalPlayer.Character.RemoteEvent:FireServer("EndDialogue",{["NPC"] = "Merchant",["Option"] = "Option1",["Dialogue"] = "Dialogue5"})
